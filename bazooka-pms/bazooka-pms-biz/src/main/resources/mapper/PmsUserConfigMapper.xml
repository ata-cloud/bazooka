<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.atayun.bazooka.pms.biz.dal.dao.PmsUserConfigMapper">
    <update id="updateConfig">
       update pms_user_config
       set config_body=#{configBody}
       where user_id=#{userId} and config_title=#{configTitle}
    </update>


</mapper>